<http://purl.org/ontology/musico#HumanConductor>
  a owl:Class ;
  owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
     <http://purl.org/ontology/mo/Conductor>
     <http://purl.org/ontology/musico#HumanMusician>
   )
  ] ;
  rdfs:subClassOf <http://purl.org/ontology/musico#HumanMusician> .

<http://purl.org/ontology/musico#HumanLiveSoundEngineer>
  a owl:Class ;
  owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
     <http://purl.org/ontology/musico#HumanMusician>
     <http://purl.org/ontology/musico#LiveSoundEngineer>
   )
  ] ;
  rdfs:subClassOf <http://purl.org/ontology/musico#HumanMusician> .

<http://purl.org/ontology/musico#HumanMusicLearner>
  a owl:Class ;
  owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
     <http://purl.org/ontology/musico#HumanMusician>
     <http://purl.org/ontology/musico#MusicLearner>
   )
  ] ;
  rdfs:subClassOf <http://purl.org/ontology/musico#HumanMusician> .

<http://purl.org/ontology/musico#HumanMusicTeacher>
  a owl:Class ;
  owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
     <http://purl.org/ontology/musico#HumanMusician>
     <http://purl.org/ontology/musico#MusicTeacher>
   )
  ] ;
  rdfs:subClassOf <http://purl.org/ontology/musico#HumanMusician> .

<http://purl.org/ontology/musico#HumanMusician>
  a owl:Class ;
  owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
     <http://purl.org/ontology/musico#Musician>
     foaf:Person
   )
  ] ;
  rdfs:subClassOf <http://purl.org/ontology/musico#Musician> ;
  owl:disjointWith <http://purl.org/ontology/musico#VirtualMusician> ;
  rdfs:comment "An actual human being involved in a musical activity (e.g., composition, performance)"@en .

<http://purl.org/ontology/musico#HumanPerformer>
  a owl:Class ;
  owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
     <http://purl.org/ontology/mo/Performer>
     <http://purl.org/ontology/musico#HumanMusician>
   )
  ] ;
  rdfs:subClassOf <http://purl.org/ontology/musico#HumanMusician> .

<http://purl.org/ontology/musico#HumanStudioProducer>
  a owl:Class ;
  owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
     <http://purl.org/ontology/musico#HumanMusician>
     <http://purl.org/ontology/musico#StudioProducer>
   )
  ] ;
  rdfs:subClassOf <http://purl.org/ontology/musico#HumanMusician> .

<http://purl.org/ontology/musico#HumanStudioProducerBedroom>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/ontology/musico#HumanStudioProducer> .

<http://purl.org/ontology/musico#HumanStudioProducerProfessional>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/ontology/musico#HumanStudioProducer> .

<http://purl.org/ontology/musico#IndoorArena>
  a owl:Class ;
  rdfs:subClassOf <https://schema.org/MusicVenue> .

<http://purl.org/ontology/musico#JamSession>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/ontology/musico/MusicalEvent> ;
  rdfs:comment """A jam session event.
        Takes as agent a musicians group."""@en .

<http://purl.org/ontology/musico#JazzClub>
  a owl:Class ;
  rdfs:subClassOf <https://schema.org/MusicVenue> .

<http://purl.org/ontology/musico#LearnersGroup>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/ontology/musico/MusiciansGroup> ;
  rdfs:comment "A group of music learners (e.g., involved in a music lesson)."@en .

<http://purl.org/ontology/musico#LiveSoundEngineer>
  a owl:Class ;
  owl:equivalentClass [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/ontology/musico#live_engineered> ;
    owl:someValuesFrom <http://purl.org/ontology/mo/Performance>
  ] ;
  rdfs:subClassOf foaf:Agent ;
  rdfs:comment "A sound engineer involved in sound engineering activities (e.g., mixing) during live music events (e.g., performances)."@en ;
  rdfs:label "LiveSoundEngineer"@en .

<http://purl.org/ontology/musico#MusicLearner>
  a owl:Class ;
  rdfs:subClassOf foaf:Agent .

<http://purl.org/ontology/musico#MusicLesson>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/ontology/musico/MusicalEvent> ;
  rdfs:comment """A music lesson event.
        Takes as agents the music teacher and music learner, as well as  teachers group and learners groups."""@en .

<http://purl.org/ontology/musico#MusicSchool>
  a owl:Class ;
  rdfs:subClassOf <https://schema.org/MusicVenue> .

<http://purl.org/ontology/musico#MusicTeacher>
  a owl:Class ;
