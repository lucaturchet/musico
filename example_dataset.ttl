<http://purl.org/vocab/bio/0.1/event> a owl:ObjectProperty .
<http://purl.org/ontology/musico#expertise_level>
  a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:domain <http://purl.org/ontology/musico#Musician> ;
  rdfs:range [
    a rdfs:Datatype ;
    owl:oneOf (
     "Beginner"
     "Expert"
     "Intermediate"
   )
  ] ;
  rdfs:comment "The expertise level of a musician (three contrained values: beginner, expert and intermediate)"@en .

<http://purl.org/ontology/musico#multi-instrumentalism_level>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/ontology/musico#Musician> ;
  rdfs:range [
    a rdfs:Datatype ;
    owl:oneOf (
     "multi-instrumentalist"
     "single-instrumentalist"
   )
  ] ;
  rdfs:comment "This property is used to describe agents able to play either one musical instrument  (single-instrumentalist) or more than one musical instrument (multi-instrumentalist)"@en .

<http://purl.org/ontology/musico#professional_level>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/ontology/musico#Musician> ;
  rdfs:range [
    a rdfs:Datatype ;
    owl:oneOf (
     "Amateur"
     "Professional"
   )
  ] ;
  rdfs:comment "The level of professionality of a musician (either amateur or professional)"@en .

<http://purl.org/ontology/musico#synchronous_asynchronous>
  a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:domain <http://purl.org/NET/c4dm/event.owl#Event> ;
  rdfs:range [
    a rdfs:Datatype ;
    owl:oneOf (
     "Asynchronous"
     "Synchronous"
   )
  ] ;
  rdfs:comment "Specifies whether virtual and/or human musicians take part to the activity at the same time or in an asynchronous way."@en .

<http://purl.org/ontology/musico/disability_type>
  a owl:DatatypeProperty ;
  rdfs:domain foaf:Person ;
  rdfs:range [
    a rdfs:Datatype ;
    owl:oneOf (
     "auditory-impaired"
     "cognitively-impaired"
     "motor-impaired"
     "visually-impaired"
   )
  ] .

<http://purl.org/ontology/musico/has_disability>
  a owl:DatatypeProperty ;
  rdfs:domain foaf:Person ;
  rdfs:range xsd:boolean .

<http://purl.org/ontology/musico/is_asynchronous>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/NET/c4dm/event.owl#Event> ;
  rdfs:range xsd:boolean ;
  owl:propertyDisjointWith <http://purl.org/ontology/musico/is_synchronous> .

<http://purl.org/ontology/musico/is_displaced>
  a owl:DatatypeProperty ;
  owl:equivalentProperty <http://purl.org/ontology/musico/is_remote> ;
  rdfs:domain <http://purl.org/NET/c4dm/event.owl#Event> ;
  rdfs:range xsd:boolean .

<http://purl.org/ontology/musico/is_in_presence>
  a owl:DatatypeProperty ;
  owl:equivalentProperty <http://purl.org/ontology/musico/is_situated> ;
  rdfs:domain <http://purl.org/NET/c4dm/event.owl#Event> ;
  rdfs:range xsd:boolean ;
  owl:propertyDisjointWith <http://purl.org/ontology/musico/is_remote> .

<http://purl.org/ontology/musico/is_multiagent_event>
  a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:subPropertyOf owl:topDataProperty ;
  rdfs:domain <http://purl.org/NET/c4dm/event.owl#Event> ;
  rdfs:range xsd:boolean ;
  owl:propertyDisjointWith <http://purl.org/ontology/musico/is_solo_event> ;
  rdfs:comment "This property is used to describe events that involve more than one agent."@en .

<http://purl.org/ontology/musico/is_remote>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/NET/c4dm/event.owl#Event> ;
  rdfs:range xsd:boolean .

<http://purl.org/ontology/musico/is_situated>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/NET/c4dm/event.owl#Event> ;
  rdfs:range xsd:boolean .

<http://purl.org/ontology/musico/is_solo_event>
  a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:subPropertyOf owl:topDataProperty ;
  rdfs:domain <http://purl.org/NET/c4dm/event.owl#Event> ;
  rdfs:range xsd:boolean ;
  rdfs:comment "This property is used to describe events that involve only one agent."@en .

<http://purl.org/ontology/musico/is_synchronous>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/NET/c4dm/event.owl#Event> ;
  rdfs:range xsd:boolean .

