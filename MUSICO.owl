        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://purl.org/ontology/musico#pl">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://purl.org/ontology/musico#a">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://purl.org/ontology/musico#b">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description>
        <rdfs:comment>Recommendation of a user based on the similarity of the genre played by the user and the genre played by others in the same city</rdfs:comment>
        <rdfs:label>userRecommendation_1</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <swrl:body>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                        <swrl:classPredicate rdf:resource="http://purl.org/ontology/musico#HumanMusician"/>
                        <swrl:argument1 rdf:resource="http://purl.org/ontology/musico#u"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                        <rdf:first>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                                <swrl:classPredicate rdf:resource="http://purl.org/ontology/musico#HumanMusician"/>
                                <swrl:argument1 rdf:resource="http://purl.org/ontology/musico#o"/>
                            </rdf:Description>
                        </rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                <rdf:first>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#BuiltinAtom"/>
                                        <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#notEqual"/>
                                        <swrl:arguments rdf:parseType="Collection">
                                            <rdf:Description rdf:about="http://purl.org/ontology/musico#u"/>
                                            <rdf:Description rdf:about="http://purl.org/ontology/musico#o"/>
                                        </swrl:arguments>
                                    </rdf:Description>
                                </rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                        <rdf:first>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                                <swrl:propertyPredicate rdf:resource="http://xmlns.com/foaf/0.1/based_near"/>
                                                <swrl:argument1 rdf:resource="http://purl.org/ontology/musico#u"/>
                                                <swrl:argument2 rdf:resource="http://purl.org/ontology/musico#city"/>
                                            </rdf:Description>
                                        </rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                                <rdf:first>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                                        <swrl:propertyPredicate rdf:resource="http://xmlns.com/foaf/0.1/based_near"/>
                                                        <swrl:argument1 rdf:resource="http://purl.org/ontology/musico#o"/>
                                                        <swrl:argument2 rdf:resource="http://purl.org/ontology/musico#city"/>
                                                    </rdf:Description>
                                                </rdf:first>
