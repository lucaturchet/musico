        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>This property models whether ther act of a musician of participating to an event is synchronous or asynchronous.</rdfs:comment>
        <rdfs:label xml:lang="en">is_synchronous</rdfs:label>
    </owl:DatatypeProperty>



    <!-- http://purl.org/ontology/musico#multi-instrumentalism_level -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/musico#multi-instrumentalism_level">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
