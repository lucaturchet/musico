        <rdfs:label xml:lang="en">is_situated</rdfs:label>
    </owl:DatatypeProperty>



    <!-- http://purl.org/ontology/musico#is_solo_event -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/musico#is_solo_event">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/c4dm/event.owl#Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">This property is used to describe events that involve only one agent.</rdfs:comment>
        <rdfs:label xml:lang="en">is_solo_event</rdfs:label>
    </owl:DatatypeProperty>



    <!-- http://purl.org/ontology/musico#is_synchronous -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/musico#is_synchronous">
