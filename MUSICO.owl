        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Relates an agent to a thing, such that the thing is recommended to the agent.</rdfs:comment>
    </owl:ObjectProperty>



    <!-- http://purl.org/ontology/musico#gets_recommended_event -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/musico#gets_recommended_event">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/musico#gets_recommendation"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/musico#HumanMusician"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/musico#MusicalEvent"/>
        <rdfs:comment>A musical event is recommended to human musician.</rdfs:comment>
    </owl:ObjectProperty>



    <!-- http://purl.org/ontology/musico#gets_recommended_genre -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/musico#gets_recommended_genre">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/musico#gets_recommendation"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/musico#HumanMusician"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Genre"/>
        <rdfs:comment>A musical genre is recommended to human musician.</rdfs:comment>
    </owl:ObjectProperty>



    <!-- http://purl.org/ontology/musico#gets_recommended_instrument -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/musico#gets_recommended_instrument">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/musico#gets_recommendation"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/musico#HumanMusician"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ontology/iomust/internet_of_things/iomust/SmartInstrument"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/mo/Instrument"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>A musical instrument (including smart instruments) is recommended to human musician.</rdfs:comment>
    </owl:ObjectProperty>



