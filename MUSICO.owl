        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/musico#HumanMusician"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/musico#MusicianParticipation"/>
        <rdfs:comment>Relates a human musician to a MusicianParticipation.</rdfs:comment>
        <rdfs:label xml:lang="en">in_participation</rdfs:label>
    </owl:ObjectProperty>



    <!-- http://purl.org/ontology/musico#interested_in -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/musico#interested_in">
        <rdfs:domain rdf:resource="http://purl.org/ontology/musico#HumanMusician"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/musico#MusicalEvent"/>
