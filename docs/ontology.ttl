#################################################################
#    Data properties
#################################################################

###  http://purl.org/ontology/musico#disability_type
:disability_type rdf:type owl:DatatypeProperty ;
                 rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                 rdfs:range [ rdf:type rdfs:Datatype ;
                              owl:oneOf [ rdf:type rdf:List ;
                                          rdf:first "auditory-impaired" ;
                                          rdf:rest [ rdf:type rdf:List ;
                                                     rdf:first "cognitively-impaired" ;
                                                     rdf:rest [ rdf:type rdf:List ;
                                                                rdf:first "motor-impaired" ;
                                                                rdf:rest [ rdf:type rdf:List ;
                                                                           rdf:first "visually-impaired" ;
                                                                           rdf:rest rdf:nil
                                                                         ]
                                                              ]
                                                   ]
                                        ]
                            ] ;
                 rdfs:label "disability_type"@en .


###  http://purl.org/ontology/musico#displayed_mood
:displayed_mood rdf:type owl:DatatypeProperty ;
                rdfs:range <http://purl.obolibrary.org/obo/MFOEM_000193> .


###  http://purl.org/ontology/musico#expertise_level
:expertise_level rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :Musician ;
                 rdfs:range [ rdf:type rdfs:Datatype ;
                              owl:oneOf [ rdf:type rdf:List ;
                                          rdf:first "Beginner" ;
                                          rdf:rest [ rdf:type rdf:List ;
                                                     rdf:first "Expert" ;
                                                     rdf:rest [ rdf:type rdf:List ;
                                                                rdf:first "Intermediate" ;
                                                                rdf:rest rdf:nil
                                                              ]
                                                   ]
                                        ]
                            ] ;
                 rdfs:comment "The expertise level of a musician (three contrained values: beginner, expert and intermediate)"@en ;
                 rdfs:label "expertise_level"@en .


###  http://purl.org/ontology/musico#expressed_mood
:expressed_mood rdf:type owl:DatatypeProperty ;
                rdfs:range <http://purl.obolibrary.org/obo/MFOEM_000193> .


###  http://purl.org/ontology/musico#felt_mood
:felt_mood rdf:type owl:DatatypeProperty ;
           rdfs:range <http://purl.obolibrary.org/obo/MFOEM_000193> .


###  http://purl.org/ontology/musico#has_disability
:has_disability rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                rdfs:range xsd:boolean ;
                rdfs:label "has_disability"@en .


###  http://purl.org/ontology/musico#is_multiagent_event
:is_multiagent_event rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf owl:topDataProperty ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain <http://purl.org/NET/c4dm/event.owl#Event> ;
                     rdfs:range xsd:boolean ;
                     owl:propertyDisjointWith :is_solo_event ;
                     rdfs:comment "This property is used to describe events that involve more than one agent."@en ;
                     rdfs:label "is_multiagent_event"@en .


###  http://purl.org/ontology/musico#is_situated
:is_situated rdf:type owl:DatatypeProperty ;
             rdfs:domain :MusicianParticipation ;
             rdfs:range xsd:boolean ;
             rdfs:comment "This property models whether ther act of a musician of participating to an event is situated (i.e., in presence) or dispalced (i.e., remote)." ;
             rdfs:label "is_situated"@en .


###  http://purl.org/ontology/musico#is_solo_event
:is_solo_event rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain <http://purl.org/NET/c4dm/event.owl#Event> ;
               rdfs:range xsd:boolean ;
               rdfs:comment "This property is used to describe events that involve only one agent."@en ;
               rdfs:label "is_solo_event"@en .


###  http://purl.org/ontology/musico#is_synchronous
:is_synchronous rdf:type owl:DatatypeProperty ;
                rdfs:domain :MusicianParticipation ;
                rdfs:range xsd:boolean ;
                rdfs:comment "This property models whether ther act of a musician of participating to an event is synchronous or asynchronous." ;
                rdfs:label "is_synchronous"@en .


###  http://purl.org/ontology/musico#multi-instrumentalism_level
:multi-instrumentalism_level rdf:type owl:DatatypeProperty ,
                                      owl:FunctionalProperty ;
                             rdfs:domain :Musician ;
                             rdfs:range [ rdf:type rdfs:Datatype ;
                                          owl:oneOf [ rdf:type rdf:List ;
                                                      rdf:first "multi-instrumentalist" ;
                                                      rdf:rest [ rdf:type rdf:List ;
                                                                 rdf:first "single-instrumentalist" ;
                                                                 rdf:rest rdf:nil
                                                               ]
                                                    ]
                                        ] ;
                             rdfs:comment "This property is used to describe agents able to play either one musical instrument  (single-instrumentalist) or more than one musical instrument (multi-instrumentalist)"@en ;
                             rdfs:label "multi-instrumentalism_level"@en .


###  http://purl.org/ontology/musico#professional_level
:professional_level rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( :Musician
                                                :MusiciansGroup
                                              )
                                ] ;
                    rdfs:range [ rdf:type rdfs:Datatype ;
                                 owl:oneOf [ rdf:type rdf:List ;
                                             rdf:first "Amateur" ;
                                             rdf:rest [ rdf:type rdf:List ;
                                                        rdf:first "Professional" ;
                                                        rdf:rest rdf:nil
                                                      ]
                                           ]
                               ] ;
                    rdfs:comment "The level of professionality of a musician (either amateur or professional)"@en ;
                    rdfs:label "professional_level"@en .


###  http://purl.org/ontology/musico#public_access
:public_access rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :MusicalEvent ;
               rdfs:range xsd:boolean ;
               rdfs:comment "A MusicalEvent may be either public or private"^^rdfs:Literal .


