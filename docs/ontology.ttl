                 rdfs:domain <http://xmlns.com/foaf/0.1/Agent> ;
                 rdfs:range <http://purl.org/ontology/mo/Performance> ;
                 rdfs:label "live_engineered"@en .


###  http://purl.org/ontology/musico#participating_musician
:participating_musician rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf <https://schema.org/participant> ;
                        rdf:type owl:AsymmetricProperty ,
                                 owl:IrreflexiveProperty ;
                        rdfs:domain :MusicianParticipation ;
                        rdfs:range :HumanMusician ;
                        rdfs:label "participating_musician"@en .


###  http://purl.org/ontology/musico#played_genre
:played_genre rdf:type owl:ObjectProperty ;
              rdfs:domain <https://schema.org/MusicVenue> ;
              rdfs:range <http://purl.org/ontology/mo/Genre> .


###  http://purl.org/ontology/musico#played_instrument
:played_instrument rdf:type owl:ObjectProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain :MusicianParticipation ;
                   rdfs:range <http://purl.org/ontology/iomust/internet_of_things/iomust/SmartInstrument> ;
                   rdfs:label "played_instrument"@en .


###  http://purl.org/ontology/musico#played_musical_work
:played_musical_work rdf:type owl:ObjectProperty ,
