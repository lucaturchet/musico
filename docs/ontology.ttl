                              owl:IrreflexiveProperty ;
                     rdfs:domain :MusicianParticipation ;
                     rdfs:range <http://purl.org/ontology/mo/MusicalWork> ;
                     rdfs:label "played_musical_work"@en .


###  http://purl.org/ontology/musico#plays_genre
:plays_genre rdf:type owl:ObjectProperty ,
                      owl:AsymmetricProperty ,
                      owl:IrreflexiveProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Musician
                                         <http://purl.org/ontology/musico/MusiciansGroup>
                                       )
                         ] ;
             rdfs:range <http://purl.org/ontology/mo/Genre> ;
             rdfs:label "plays_genre"@en .


###  http://purl.org/ontology/musico#plays_instrument
:plays_instrument rdf:type owl:ObjectProperty ,
                           owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain :Musician ;
                  rdfs:range <http://purl.org/ontology/mo/Instrument> ;
                  rdfs:label "plays_instrument"@en .


###  http://purl.org/ontology/musico#recognized_emotion
:recognized_emotion rdf:type owl:ObjectProperty ,
                             owl:AsymmetricProperty ,
                             owl:IrreflexiveProperty ;
                    rdfs:domain :MusicianParticipation ;
                    rdfs:range <http://purl.obolibrary.org/obo/MFOEM_000001> ;
                    rdfs:label "recognized_emotion"@en .


###  http://purl.org/ontology/musico#recognized_mood
:recognized_mood rdf:type owl:ObjectProperty ;
