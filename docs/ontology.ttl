                          owl:IrreflexiveProperty ;
                 rdfs:domain :MusicianParticipation ;
                 rdfs:range <http://purl.obolibrary.org/obo/MFOEM_000193> ;
                 rdfs:label "recognized_mood"@en .


###  http://purl.org/ontology/musico#sub_genre
:sub_genre rdf:type owl:ObjectProperty ,
                    owl:AsymmetricProperty ,
                    owl:IrreflexiveProperty ;
           rdfs:domain <http://purl.org/ontology/mo/Genre> ;
           rdfs:range <http://purl.org/ontology/mo/Genre> ;
           rdfs:comment "A property relating a genre as a subgenre of another. E.g., Bop or Swing are sub-genres of the Jazz genre."@en ;
           rdfs:label "sub_genre"@en .


###  http://purl.org/ontology/musico#taught_musical_work
:taught_musical_work rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     rdf:type owl:AsymmetricProperty ,
                              owl:IrreflexiveProperty ;
                     rdfs:domain :MusicianParticipation ;
                     rdfs:range <http://purl.org/ontology/mo/MusicalWork> ;
                     rdfs:label "taught_musical_work"@en .


###  http://purl.org/ontology/musico#teaches_music_at
:teaches_music_at rdf:type owl:ObjectProperty ,
