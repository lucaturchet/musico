               owl:IrreflexiveProperty ;
      rdfs:domain :HumanMusician ;
      rdfs:range :MusicalService ;
      rdfs:comment "Property that models the fact that a human musician utilizes some kind of musical services."@en ;
      rdfs:label "uses"@en .


