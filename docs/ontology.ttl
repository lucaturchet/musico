###  http://purl.org/ontology/musico#gets_recommendation
:gets_recommendation rdf:type owl:ObjectProperty ;
                     rdfs:domain <http://xmlns.com/foaf/0.1/Agent> ;
                     rdfs:range owl:Thing .


###  http://purl.org/ontology/musico#gets_recommended_event
:gets_recommended_event rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :gets_recommendation ;
                        rdfs:domain :HumanMusician ;
                        rdfs:range :MusicalEvent .


###  http://purl.org/ontology/musico#gets_recommended_genre
:gets_recommended_genre rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :gets_recommendation ;
                        rdfs:domain :HumanMusician ;
                        rdfs:range <http://purl.org/ontology/mo/Genre> .


###  http://purl.org/ontology/musico#gets_recommended_instrument
:gets_recommended_instrument rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf :gets_recommendation ;
                             rdfs:domain :HumanMusician ;
                             rdfs:range [ rdf:type owl:Class ;
                                          owl:unionOf ( <http://purl.org/ontology/iomust/internet_of_things/iomust/SmartInstrument>
                                                        <http://purl.org/ontology/mo/Instrument>
                                                      )
                                        ] .


###  http://purl.org/ontology/musico#gets_recommended_music_venue
:gets_recommended_music_venue rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf :gets_recommendation ;
                              rdfs:domain :HumanMusician ;
                              rdfs:range <https://schema.org/MusicVenue> .


###  http://purl.org/ontology/musico#gets_recommended_user
:gets_recommended_user rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :gets_recommendation ;
                       rdfs:domain <http://xmlns.com/foaf/0.1/Agent> ;
                       rdfs:range <http://xmlns.com/foaf/0.1/Agent> .


###  http://purl.org/ontology/musico#has_group
:has_group rdf:type owl:ObjectProperty ,
                    owl:AsymmetricProperty ,
                    owl:IrreflexiveProperty ;
           rdfs:domain :MusicalEvent ;
           rdfs:range :MusiciansGroup ;
           rdfs:label "has_group"@en .


###  http://purl.org/ontology/musico#has_virtual_musician
:has_virtual_musician rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdf:type owl:AsymmetricProperty ,
                               owl:IrreflexiveProperty ;
                      rdfs:domain <http://purl.org/ontology/iomust/smi#SMIApplication> ;
                      rdfs:range :VirtualMusician ;
                      rdfs:label "has_virtual_musician"@en .


###  http://purl.org/ontology/musico#in_participation
:in_participation rdf:type owl:ObjectProperty ;
                  owl:inverseOf :participating_musician ;
                  rdf:type owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain :HumanMusician ;
                  rdfs:range :MusicianParticipation ;
                  rdfs:label "in_participation"@en .


###  http://purl.org/ontology/musico#interested_in
:interested_in rdf:type owl:ObjectProperty ;
               rdfs:domain :HumanMusician ;
               rdfs:range :MusicalEvent .


###  http://purl.org/ontology/musico#involved_event
:involved_event rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ,
                         owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain :MusicianParticipation ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( :MusicalEvent
                                           :MusicalSession
                                         )
                           ] ;
                rdfs:label "involved_event"@en .


###  http://purl.org/ontology/musico#is_mentor_of
:is_mentor_of rdf:type owl:ObjectProperty ;
              owl:inverseOf :is_pupil_of ;
              rdfs:domain :MusicTeacher ;
              rdfs:range :MusicLearner ;
              rdfs:label "is_mentor_of"@en .


###  http://purl.org/ontology/musico#is_pupil_of
:is_pupil_of rdf:type owl:ObjectProperty ;
             rdfs:domain :MusicLearner ;
             rdfs:range :MusicTeacher ;
             rdfs:label "is_pupil_of"@en .


###  http://purl.org/ontology/musico#learns_music_at
:learns_music_at rdf:type owl:ObjectProperty ,
                          owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain :MusicLearner ;
                 rdfs:range <https://schema.org/MusicVenue> ;
                 rdfs:label "learns_music_at"@en .


###  http://purl.org/ontology/musico#learnt_musical_work
:learnt_musical_work rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     rdf:type owl:AsymmetricProperty ,
                              owl:IrreflexiveProperty ;
                     rdfs:domain :MusicianParticipation ;
                     rdfs:range <http://purl.org/ontology/mo/MusicalWork> ;
                     rdfs:label "learnt_musical_work"@en .


###  http://purl.org/ontology/musico#live_engineer
:live_engineer rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://purl.org/NET/c4dm/event.owl#agent> ;
               owl:inverseOf :live_engineered ;
               rdfs:domain <http://purl.org/ontology/mo/Performance> ;
               rdfs:range <http://xmlns.com/foaf/0.1/Agent> .


###  http://purl.org/ontology/musico#live_engineered
:live_engineered rdf:type owl:ObjectProperty ;
